<?xml version="1.0" encoding="utf-8"?>

<shape
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval"
        android:useLevel="false"
        android:innerRadiusRatio="7"
        android:thicknessRatio="3"
>
    <size
            android:width="360dp"
            android:height="360dp"/>

    <stroke
            android:width="100dp"
            android:dashWidth="31.4dp"
            android:dashGap="31.4dp"
            android:color="@color/color_icon_stroke"
    />

    <gradient
            android:startColor="@color/color_icon_bg"
            android:type="radial"
            android:gradientRadius="160dp"
    />

</shape>